  <property name="styleSheet">
   <string notr="true">QMainWindow, QWidget {
    /* The main background color - A deep Ubuntu-ish purple-grey */
    background-color: #282a36; 
    /* Main text color - Off-white for good contrast without eye strain */
    color: #f8f8f2;
    /* Optional: Sets a modern system font */
    font-family: &quot;Segoe UI&quot;;
    font-size: 10pt; /* Slightly larger base font for modern feel */
}

/* =======================================================
   INPUTS &amp; TEXT BOXES (The &quot;Gemini&quot; look)
   Very rounded corners, slightly lighter background so they pop.
   ======================================================= */
QLineEdit, QSpinBox, QDoubleSpinBox, QTextEdit, QComboBox, QPlainTextEdit {
    /* Lighter background than the main window to stand out */
    background-color: #343746; 
    /* A subtle border defining the shape */
    border: 2px solid #44475a; 
    /* HEAVY rounding for that modern flowy look */
    border-radius: 8px; 
    /* Crucial padding so text doesn't touch the edges */
    padding: 6px 10px; 
    color: #f8f8f2;
    /* Purple selection color */
    selection-background-color: #bd93f9; 
    selection-color: #282a36;
}

/* The &quot;Splash&quot; effect when you click inside an input box */
QLineEdit:focus, QSpinBox:focus, QTextEdit:focus, QComboBox:focus {
    /* Bright Ubuntu purple border when active */
    border: 2px solid #bd93f9; 
    background-color: #3b3e4f; /* Slightly lighter splash */
}

/* Spinbox up/down arrows need specific styling so they don't look blocky */
QAbstractSpinBox::up-button, QAbstractSpinBox::down-button {
    background-color: transparent;
    border-radius: 6px;
    margin: 2px;
}
QAbstractSpinBox::up-button:hover, QAbstractSpinBox::down-button:hover {
    background-color: #44475a;
}


/* =======================================================
   BUTTONS (The Ubuntu Accent)
   Purple splash, very round.
   ======================================================= */
QPushButton {
    /* The main Ubuntu purple accent color */
    background-color: #6272a4; 
    color: #ffffff;
    border: none;
    
    /* REDUCED ROUNDING: 12px is too big for small screens */
    border-radius: 6px; 
    
    /* REDUCED PADDING: This was the main cause of the height issue */
    padding: 5px 15px; 
    
    font-weight: bold;
}

/* Hover state - gets brighter */
QPushButton:hover {
    background-color: #bd93f9; /* Lighter bright purple */
}

/* Pressed state - gets darker */
QPushButton:pressed {
    background-color: #4f5b85;
}

/* Disabled state */
QPushButton:disabled {
    background-color: #343746;
    color: #6272a4;
    border: 1px solid #44475a;
}


/* =======================================================
   CONTAINERS &amp; LABELS
   Removing the old &quot;boxy&quot; look.
   ======================================================= */

/* Clean Labels */
QLabel {
    color: #f8f8f2;
    border: none;
    padding: 2px;
}

/* Modernizing Group Boxes */
QGroupBox {
    /* subtle border instead of the default etched line */
    border: 1px solid #6272a4; 
    border-radius: 12px;
    /* Push the content down to make room for the title */
    margin-top: 24px; 
    padding-top: 10px;
    font-weight: bold;
}

/* Styling the GroupBox Title text */
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 12px;
    padding: 0 5px;
    color: #bd93f9; /* Purple title color */
}


/* =======================================================
   SCROLLBARS (Optional, but completes the look)
   Making them dark and slim.
   ======================================================= */
QScrollBar:vertical {
    border: none;
    background: #282a36;
    width: 10px;
    margin: 0px;
}
QScrollBar::handle:vertical {
    background: #44475a;
    border-radius: 5px;
    min-height: 20px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}

/* =======================================================
   TABS 
   Fixing the white background issue
   ======================================================= */
QTabWidget::pane {
    /* The body of the tab widget */
    border: 1px solid #44475a;
    background-color: #282a36;
}

QTabBar::tab {
    /* The clickable tabs at the top */
    background-color: #343746;
    color: #f8f8f2;
    padding: 8px 20px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    /* The active tab */
    background-color: #6272a4;
    font-weight: bold;
}

QTabBar::tab:hover {
    background-color: #44475a;
}</string>
